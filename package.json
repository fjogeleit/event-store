    "@types/uuid": "^3.4.8",
    "fastify": "^2.12.0",
    "fastify-postgres": "^2.0.0",
    "jest": "24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "1.18.2",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "24.1.0",
    "typescript": "^3.8.2"
